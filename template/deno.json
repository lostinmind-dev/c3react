{
    "name": "@c3react/template",
    "version": "1.0.0",
    "compilerOptions": {
        "types": [
            "./ts-defs.d.ts"
        ],
        "lib": [
            "deno.ns",
            "dom",
            "dom.asynciterable",
            "dom.iterable"
        ]
    },
    "tasks": {
        "bundle:tsdefs": "",
        "dev": "vite build --watch",
        "build:dev": "deno bundle --output ./project/scripts/main.js ./scripts/main.ts",
        "build:prod": "deno bundle --output ./project/scripts/main.js --minify ./scripts/main.ts"
    },
    "imports": {
        "c3react": "../mod.ts",

        "@/": "./scripts/"
    }
}